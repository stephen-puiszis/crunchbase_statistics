<div class="label">
    <h3 class= "container">Listing financials</h1>
</div>

<table>
  <tr>
    <th>Name</th>
    <th>Perma</th>
    <th>Crunch url</th>
    <th>Home url</th>
    <th>Founded date</th>
  </tr>

<% @financials.each do |financial| %>
  <tr>
    <td><%= link_to financial.name, financial %></td>
    <td><%= financial.perma %></td>
    <td><%= financial.crunch_url %></td>
    <td><%= financial.home_url %></td>
    <td><%= financial.founded_date %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Financial', new_financial_path %>
